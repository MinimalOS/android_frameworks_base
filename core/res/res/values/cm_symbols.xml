    <!-- Package Manager -->
    <java-symbol type="array" name="config_disabledComponents" />
    <java-symbol type="array" name="config_forceEnabledComponents" />

    <!-- Stylus gestures -->
    <java-symbol type="bool" name="config_stylusGestures" />
    <java-symbol type="string" name="stylus_app_not_installed" />

    <!-- Ignored sms packages -->
    <java-symbol type="array" name="config_ignored_sms_packages" />

    <!-- Blur effects -->
    <java-symbol type="bool" name="config_ui_blur_enabled" />

    <!-- Storage -->
    <java-symbol type="string" name="storage_usb" />
    <java-symbol type="string" name="storage_uicc" />
